    "@types/luxon": "^1.15.2",
    "handlebars": "^4.1.2",
    "jest": "^23.6.0",
    "js-yaml": "^3.13.1",
    "jsonwebtoken": "^9.0.0",
    "prettier": "^1.15.3",
    "sshpk": "^1.13.2",
    "ts-jest": "^23.10.5",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
